<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
   <data noupdate="1">

       <record id="ir_sequence_document_hub_document" model="ir.sequence">
           <field name="name">Document hub: sequence number</field>
           <field name="code">document_hub.document</field>
           <field name="prefix">DOC/%(year)s/%(month)s/</field>
           <field name="padding">4</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" ref="base.main_company"/>
       </record>

       <record id="ir_sequence_technology_document_hub_document" model="ir.sequence">
           <field name="name">Document hub technology: sequence number</field>
           <field name="code">document_hub.document</field>
           <field name="prefix">DOC-T/%(year)s/%(month)s/</field>
           <field name="padding">4</field>
           <field name="number_next">1</field>
           <field name="number_increment">1</field>
           <field name="company_id" eval="2"/>
       </record>

   </data>
</odoo>
